# Example credentials.yaml file for Redmine MCP Extension
# Copy this file to credentials.yaml and update with your actual credentials

# Redmine API settings
# For local Docker setup, use http://localhost:3000 or http://redmine:3000 from within the Docker network
redmine_url: 'http://localhost:3000'
# Generate this key in Redmine: My account > API access key
redmine_api_key: 'your_redmine_api_key_here'

# Claude API settings
# Get an API key from https://console.anthropic.com/
claude_api_key: 'your_claude_api_key_here'

# Rate limiting settings
rate_limit_per_minute: 60