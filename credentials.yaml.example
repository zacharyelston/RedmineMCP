# Example credentials file for Redmine MCP extension
# Copy this file to credentials.yaml and update with your actual values

# Redmine API configuration
redmine_url: 'https://your-redmine-server.com'  # URL of your Redmine instance (without trailing slash)
redmine_api_key: 'your_redmine_api_key_here'    # Your Redmine API key

# Claude API configuration (currently using Claude 3 models)
claude_api_key: 'your_claude_api_key_here'      # Your Claude API key

# Rate limiting
rate_limit_per_minute: 60  # Maximum API calls per minute